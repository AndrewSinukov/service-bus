{
  "name": "mmasiukevich/service-bus",
  "description": "PHP Service Bus (CQS implementation)",
  "type": "library",
  "authors": [
    {
      "name": "Masiukevich Maksim",
      "email": "desperado@minsk-info.ru",
      "homepage": "https://github.com/mmasiukevich",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Desperado\\ServiceBus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Desperado\\ServiceBus\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1",
    "mmasiukevich/infrastructure": "^v0.0.3",
    "mmasiukevich/message-serializer": "^v0.0.3",
    "mmasiukevich/event-sourcing": "^v0.0.3",
    "symfony/dotenv": "v4.0.4",
    "symfony/dependency-injection": "v4.0.4",
    "symfony/config": "v4.0.4",
    "symfony/validator": "v4.0.4",
    "symfony/event-dispatcher": "v4.0.4",
    "doctrine/cache": "v1.7.1",
    "phpdocumentor/reflection-docblock": "4.2.0",
    "react/http": "v0.8.1",
    "react/child-process": "v0.5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "symfony/debug": "v4.0.4"
  },
  "config": {
    "optimize-autoloader": true
  },
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "v0.0.3-dev"
    }
  },
  "scripts": {
    "post-autoload-dump": "curl -s -H \"Accept: text/plain\" https://security.sensiolabs.org/check_lock -F lock=@composer.lock"
  }
}
