{
  "name": "desperado/php-es-cqrs",
  "description": "Command Query Responsibility Segregation/Event Sourcing implementation",
  "type": "library",
  "keywords": [
    "CQRS",
    "Command Query Responsibility Segregation",
    "Event Sourcing",
    "Concurrency",
    "Framework",
    "Async PHP"
  ],
  "authors": [
    {
      "name": "Masiukevich Maksim",
      "email": "desperado@minsk-info.ru",
      "homepage": "https://github.com/mmasiukevich",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Desperado\\Framework\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Desperado\\Framework\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1",
    "monolog/monolog": "1.23.0",
    "symfony/yaml": "^3.3",
    "symfony/dotenv": "^3.3",
    "symfony/serializer": "v3.3.6",
    "symfony/property-info": "v3.3.6",
    "symfony/property-access": "v3.3.6",
    "doctrine/annotations": "v1.5.0",
    "ramsey/uuid": "3.7.0",
    "bunny/bunny": "^0.2.4",
    "voryx/event-loop": "^2.0",
    "react/react": "v0.4.2",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2"
  },
  "config": {
    "bin-dir": "bin",
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-develop": "0.1-dev"
    }
  }
}
