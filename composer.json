{
  "name": "desperado/concurrency-framework",
  "description": "CQRS/Event Sourcing Non-blocking concurrency framework",
  "type": "library",
  "keywords": [
    "CQRS",
    "Command Query Responsibility Segregation",
    "Event Sourcing",
    "Concurrency",
    "Framework"
  ],
  "authors": [
    {
      "name": "Masiukevich Maksim",
      "email": "desperado@minsk-info.ru",
      "homepage": "https://github.com/mmasiukevich",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Desperado\\ConcurrencyFramework\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Desperado\\ConcurrencyFramework\\Tests\\": "tests/src/"
    }
  },
  "require": {
    "php": ">=7.1",
    "monolog/monolog": "1.23.0",
    "symfony/yaml": "^3.3",
    "symfony/dotenv": "^3.3",
    "symfony/serializer": "v3.3.6",
    "symfony/property-info": "v3.3.6",
    "symfony/property-access": "v3.3.6",
    "amphp/redis": "0.3.0",
    "doctrine/annotations": "v1.5.0",
    "ramsey/uuid": "3.7.0",
    "amphp/postgres": "^0.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2"
  },
  "config": {
    "bin-dir": "bin",
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-develop": "0.1-dev"
    }
  }
}
