{
  "name": "mmasiukevich/framework",
  "description": "Command Query Responsibility Segregation/Event Sourcing implementation",
  "type": "library",
  "keywords": [
    "CQRS",
    "Command Query Responsibility Segregation",
    "Event Sourcing",
    "Concurrency",
    "Framework",
    "Async PHP"
  ],
  "authors": [
    {
      "name": "Masiukevich Maksim",
      "email": "desperado@minsk-info.ru",
      "homepage": "https://github.com/mmasiukevich",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Desperado\\Framework\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Desperado\\Framework\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1",
    "mmasiukevich/infrastructure": "^0.1",
    "mmasiukevich/event-sourcing-saga": "^0.1",
    "mmasiukevich/event-storage-pgsql": "^0.1",
    "mmasiukevich/message-serializer": "^0.1",
    "mmasiukevich/event-sourcing": "^0.1",
    "mmasiukevich/domain-common": "^0.1",
    "mmasiukevich/cqrs": "^0.1",
    "bunny/bunny": "^0.2.4",
    "symfony/dotenv": "^3.3.6",
    "influxdb/influxdb-php": "1.14.4",
    "react/react": "v0.4.*",
    "zendframework/zend-diactoros": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2"
  },
  "suggest": {
    "influxdata/influxdb-php": "Provide InfluxDb metrics storage"
  },
  "config": {
    "bin-dir": "bin",
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
