{
  "name": "mmasiukevich/service-bus",
  "description": "PHP Service Bus (CQS implementation)",
  "type": "library",
  "authors": [
    {
      "name": "Masiukevich Maksim",
      "email": "desperado@minsk-info.ru",
      "homepage": "https://github.com/mmasiukevich",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Desperado\\ServiceBus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Desperado\\ServiceBus\\Demo\\": "demo/src/",
      "Desperado\\ServiceBus\\Tests\\": "tests/",
      "Desperado\\Domain\\Tests\\": "vendor/mmasiukevich/domain-common/tests"
    }
  },
  "require": {
    "php": ">=7.1",
    "mmasiukevich/infrastructure": "^0.1",
    "mmasiukevich/saga": "^0.1",
    "mmasiukevich/message-serializer": "^0.1",
    "bunny/bunny": "^0.2.4",
    "symfony/dotenv": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/config": "^4.0",
    "symfony/validator": "^4.0",
    "symfony/serializer": "^4.0",
    "symfony/property-info": "^4.0",
    "symfony/property-access": "^4.0",
    "symfony/event-dispatcher": "^4.0",
    "react/http": "v0.7.4",
    "doctrine/annotations": "v1.5.0",
    "doctrine/cache": "v1.7.1",
    "phpdocumentor/reflection-docblock": "4.2.0"
  },
  "require-dev": {
    "mmasiukevich/event-sourcing": "^0.1",
    "phpunit/phpunit": "^6.2",
    "symfony/debug": "^4.0"
  },
  "config": {
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "bin-dir": "bin",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  }
}
